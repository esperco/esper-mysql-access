OCAML_LIBS = $(OCAML_LIBS_mysql-access)

FILES[] =
  mysql_util
  mysql_types
  mysql_lwt
  mysql_access_kv
  mysql_access_kkv
  mysql_access_kk
  mysql_access_util

OCamlLibrary(mysql-access, $(FILES))
Meta(mysql-access)

.DEFAULT: META mysql-access.cma mysql-access.cmxa

.PHONY: clean
clean:
  rm -f *.o *.a *.cm* *~ *.annot *.run *.opt META $(ATDGEN_OUTFILES)
